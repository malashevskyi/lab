{
  "name": "@lab/types",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -b tsconfig.lib.json",
    "generate:zod": "ts-to-zod --all"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./assistant/tts/index.js": {
      "types": "./dist/assistant/tts/index.d.ts",
      "require": "./dist/assistant/tts/index.cjs",
      "import": "./dist/assistant/tts/index.js"
    },
    "./assistant/dictionary-examples/index.js": {
      "types": "./dist/assistant/dictionary-examples/index.d.ts",
      "require": "./dist/assistant/dictionary-examples/index.cjs",
      "import": "./dist/assistant/dictionary-examples/index.js"
    },
    "./assistant/dictionary-entries/index.js": {
      "types": "./dist/assistant/dictionary-entries/index.d.ts",
      "require": "./dist/assistant/dictionary-entries/index.cjs",
      "import": "./dist/assistant/dictionary-entries/index.js"
    },
    "./assistant/ai/index.js": {
      "types": "./dist/assistant/ai/index.d.ts",
      "require": "./dist/assistant/ai/index.cjs",
      "import": "./dist/assistant/ai/index.js"
    },
    "./assistant/audio-records/index.js": {
      "types": "./dist/assistant/audio-records/index.d.ts",
      "require": "./dist/assistant/audio-records/index.cjs",
      "import": "./dist/assistant/audio-records/index.js"
    },
    "./assistant/flashcards/index.js": {
      "types": "./dist/assistant/flashcards/index.d.ts",
      "require": "./dist/assistant/flashcards/index.cjs",
      "import": "./dist/assistant/flashcards/index.js"
    }
  },
  "dependencies": {
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "ts-to-zod": "^5.0.1"
  }
}
